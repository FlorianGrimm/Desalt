{
  "compilerOptions": {
    "baseUrl": "custom-types",
    "forceConsistentCasingInFileNames": true,
    "jsx": "react",
    "lib": ["dom", "es5", "es2015.collection"],
    "module": "commonjs",
    "newLine": "LF",
    "outDir": "lib",
    "sourceMap": true,
    "strict": true,
    "target": "es5",
    "typeRoots": ["custom-types", "node_modules/@types"]
  },
  "include": ["**/*.ts"]
}
