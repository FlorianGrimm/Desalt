{
  "outputPath": ".",
  "warningLevel": "informational",
  "generalDiagnosticOption": "default",
  "specificDiagnosticOptions": {
    "DSC1003": "suppress",
    "DSC1005": "suppress",
    "DSC1012": "suppress",
    "DSC1028": "suppress"
  },
  "renameRules": {
